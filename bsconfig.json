{
  "name": "nextjs-default",
  "namespace": true,
  "reason": { "react-jsx": 3 },
  "refmt": 3,
  "bs-dependencies": [
    "bs-fetch",
    "@glennsl/bs-json",
    "bs-onegraph-auth",
    "reason-react",
    "reason-relay",
    "reason-promise"
  ],
  "ppx-flags": ["reason-relay/ppx"],
  "sources": [
    { "dir": "common", "subdirs": true },
    { "dir": "bindings" },
    { "dir": "pages", "subdirs": true },
    { "dir": "components", "subdirs": true },
    "layouts"
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors", "-bs-g"],
  "gentypeconfig": {
    "language": "untyped",
    "shims": [],
    "module": "es6"
  }
}
